  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.0",
    "light-my-request": "^4.4.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
